{
  "manifest_version": 2,
  "name": "Assistant",
  "version": "0.0.1",
  "permissions": [
    "storage",
    "webRequest",
    "webRequestBlocking",
    "*://twitter.com/*",
    "*://www.twitter.com/*",
    "*://instagram.com/*",
    "*://www.instagram.com/*",
    "*://facebook.com/*",
    "*://www.facebook.com/*",
    "*://reddit.com/*",
    "*://www.reddit.com/*",
    "*://9gag.com/*",
    "*://www.9gag.com/*",
    "*://netflix.com/*",
    "*://www.netflix.com/*",
    "*://disneyplus.com/*",
    "*://www.disneyplus.com/*",
    "*://videoland.com/*",
    "*://www.videoland.com/*",
    "*://youtube.com/*",
    "*://www.youtube.com/*",
    "*://tiktok.com/*",
    "*://www.tiktok.com/*",
    "*://nu.nl/*",
    "*://www.nu.nl/*",
    "*://vi.nl/*",
    "*://www.vi.nl/*",
    "*://ad.nl/*",
    "*://www.ad.nl/*",
    "*://nos.nl/*",
    "*://www.nos.nl/*",
    "*://zalando.nl/*",
    "*://www.zalando.nl/*",
    "*://fd.nl/*",
    "*://www.fd.nl/*",
    "*://tweakers.net/*",
    "*://www.tweakers.net/*",
    "*://linkedin.com/*",
    "*://www.linkedin.com/*",
    "*://asos.com/*",
    "*://www.asos.com/*",
    "*://zara.com/*",
    "*://www.zara.com/*",
    "*://bol.com/*",
    "*://www.bol.com/*",
    "*://amazon.nl/*",
    "*://www.amazon.nl/*",
    "*://amazon.com/*",
    "*://www.amazon.com/*",
    "*://coolblue.nl/*",
    "*://www.coolblue.nl/*"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "description": "Bring focus to your life and improve your productivity!",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_icon": "icon128.png",
    "default_title": "Assistant",
    "default_popup": "popup_focus.html"
  }
}

